{
  "name": "react-starterkit",
  "version": "4.0.0",
  "description": "React starterkit with Koa",
  "author": "Alberto Gasparin",
  "keywords": [
    "react",
    "koa",
    "starter kit",
    "redux",
    "router"
  ],
  "private": true,
  "config": {},
  "scripts": {
    "start": "node ./server.js",
    "prebuild": "npm run clean -s",
    "build": "npm run build:assets -s -- --progress --hide-modules && npm run build:template -s",
    "build:assets": "NODE_ENV=production webpack",
    "build:template": "NODE_ENV=production node ./tools/static-builder",
    "pretest": "npm run lint -s",
    "test": "npm run test:unit -s",
    "test:unit": "NODE_ENV=test jest --no-cache --coverage",
    "test:unit:watch": "NODE_ENV=test jest --watch",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore --ignore-pattern \"public/**\" . && stylelint --syntax scss \"app/**/*.scss\"",
    "clean": "rm -rf ./public/assets && rm -rf ./coverage && rm -rf ./.nyc_output",
    "prewatch": "npm run clean -s",
    "watch": "supervisor -q -w lib,api,www,webpack.config.js -e js,marko -i __tests__ -- --inspect ./server.js"
  },
  "jest": {
    "testMatch": ["**/__tests__/unit*.js"],
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|webp|svg|woff2?|mp{2,4}|webm|m4a|aac)$": "<rootDir>/tests/unit/fileMock.js",
      "\\.s?css$": "<rootDir>/tests/unit/fileMock.js"
    },
    "clearMocks": true,
    "resetMocks": true,
    "resetModules": true,
    "setupTestFrameworkScriptFile": "<rootDir>/tests/unit/setup.js",
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": ["/node_modules/", "/__tests__/", "/tests/"]
  },
  "dependencies": {
    "autoprefixer": "^6.7.0",
    "axios": "^0.15.0",
    "babel-core": "^6.23.0",
    "babel-loader": "^6.3.0",
    "babel-plugin-lodash": "^3.2.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.23.0",
    "co": "^4.4.0",
    "css-loader": "^0.14.5",
    "dotenv": "^4.0.0",
    "exports-loader": "^0.6.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "happypack": "^3.0.0",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.7.0",
    "koa": "^1.2.0",
    "koa-better-body": "^3.0.0",
    "koa-compress": "^1.0.0",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^3.0.0",
    "koa-router": "^5.4.0",
    "koa-static-cache": "^4.0.0",
    "lodash": "^4.1.0",
    "marko": "^3.14.0",
    "node-sass": "^4.5.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.3.0",
    "promise-polyfill": "^6.0.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^5.0.0",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-thunk": "^2.2.0",
    "require-hacker": "~2.1.0",
    "reselect": "^2.5.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.0",
    "supertest": "^3.0.0",
    "svg-sprite-loader": "^0.3.0",
    "url-loader": "^0.5.0",
    "webpack": "^2.2.0",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.16.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-react": "^6.10.0",
    "jasmine-co": "^1.2.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.0",
    "react-transform-hmr": "^1.0.0",
    "redux-immutable-state-invariant": "^1.2.0",
    "stylelint": "^7.9.0",
    "supervisor": "^0.12.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  }
}
