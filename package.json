{
  "name": "react-starterkit",
  "version": "4.0.0",
  "description": "React starterkit with Koa",
  "author": "Alberto Gasparin",
  "keywords": ["react", "koa", "starter kit", "redux", "router"],
  "private": true,
  "config": {
    "testfiles": "find ./tests . -name unit*.js -not -path */node_modules/*",
    "mochaargs": "--compilers js:babel-register --require ./lib/react-compat --require ./tests/unit/setup"
  },
  "scripts": {
    "start": "node -r babel-register ./server.js",
    "prebuild": "npm run clean -s",
    "build": "npm run build:assets -s -- --progress --hide-modules && npm run build:template -s",
    "build:assets": "NODE_ENV=production webpack",
    "build:template": "NODE_ENV=production node ./tools/static-builder",
    "pretest": "npm run lint -s",
    "test": "npm run test:unit -s",
    "test:unit": "NODE_ENV=test mocha $($npm_package_config_testfiles | awk '!x[$0]++') $npm_package_config_mochaargs",
    "test:unit:watch": "npm run test:unit -s -- --watch --watch-extensions jsx,json --reporter min",
    "coverage": "nyc npm run test:unit -s -- --reporter dot",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore --ignore-pattern \"public/**\" . && prettier --single-quote --trailing-comma es5 --list-different \"{api,api-socket,app,lib,tests,www}/**/*.{js,scss}\" && stylelint --syntax scss \"app/**/*.scss\"",
    "clean": "rm -rf ./public/assets && rm -rf ./coverage && rm -rf ./.nyc_output",
    "prewatch": "npm run clean -s",
    "watch": "supervisor -q -w lib,api,www,webpack.config.js -e js,marko -i __tests__ -- --inspect -r babel-register ./server.js"
  },
  "nyc": {
    "require": ["babel-register", "./lib/react-compat"],
    "exclude": [
      "node_modules",
      "webpack.config.js",
      "tests",
      "**/__tests__/**"
    ],
    "reporter": ["lcov", "text"],
    "cache": true,
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "autoprefixer": "^7.0.0",
    "axios": "~0.16.2",
    "babel-core": "~6.25.0",
    "babel-loader": "~7.1.1",
    "babel-plugin-lodash": "^3.2.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "~0.4.6",
    "babel-preset-env": "~1.6.0",
    "babel-preset-react": "^6.24.0",
    "babel-preset-stage-2": "^6.24.0",
    "babel-register": "^6.24.0",
    "co": "^4.4.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "exports-loader": "^0.6.0",
    "extract-text-webpack-plugin": "~2.1.2",
    "file-loader": "~0.11.2",
    "http-errors": "~1.6.1",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.7.0",
    "koa": "~2.3.0",
    "koa-better-body": "^3.0.0",
    "koa-compress": "~2.0.0",
    "koa-convert": "~1.2.0",
    "koa-logger": "~3.0.1",
    "koa-onerror": "^3.0.0",
    "koa-router": "~7.2.1",
    "koa-static-cache": "~5.1.1",
    "lodash": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.0",
    "marko": "~4.4.17",
    "node-sass": "^4.5.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "pify": "~3.0.0",
    "postcss-loader": "~2.0.6",
    "prettier": "~1.5.2",
    "promise-polyfill": "^6.0.0",
    "prop-types": "^15.5.10",
    "react": "~15.6.1",
    "react-dom": "~15.6.1",
    "react-redux": "^5.0.0",
    "react-router": "^3.0.0",
    "redux": "~3.7.1",
    "redux-actions": "~2.2.1",
    "redux-thunk": "^2.2.0",
    "require-hacker": "^3.0.0",
    "reselect": "^3.0.0",
    "sass-loader": "~6.0.6",
    "style-loader": "~0.18.2",
    "svg-sprite-loader": "~3.0.5",
    "url-loader": "~0.5.9",
    "webpack": "~3.0.0",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "babel-plugin-istanbul": "~4.1.4",
    "babel-plugin-react-transform": "^2.0.0",
    "chai": "~4.0.2",
    "enzyme": "~2.9.1",
    "eslint": "~4.1.1",
    "eslint-import-resolver-webpack": "~0.8.3",
    "eslint-plugin-import": "~2.6.1",
    "eslint-plugin-mocha": "~4.11.0",
    "eslint-plugin-react": "~7.1.0",
    "mocha": "~3.4.2",
    "nyc": "~11.0.3",
    "react-test-renderer": "~15.6.1",
    "react-transform-hmr": "^1.0.0",
    "stylelint": "~7.12.0",
    "supertest": "^3.0.0",
    "supervisor": "^0.12.0",
    "testdouble": "~3.2.2",
    "webpack-dev-middleware": "~1.11.0",
    "webpack-hot-middleware": "~2.18.1"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
